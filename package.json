{
  "name": "pl-vue",
  "version": "0.1.0",
  "description": "Plagiarize Vue",
  "main": "publish/index.js",
  "scripts": {
    "dev": "vite -c build/vite.client.ts",
    "preview": "vite preview -c build/vite.client.ts",
    "build:client": "vite build -c build/vite.client.ts",
    "build:server": "vite build -c build/vite.server.ts",
    "build": "rm -rf dist/ && npm run build:client && npm run build:server",
    "start": "node dist/server.js",
    "build:publish": "vite build -c build/vite.publish.ts"
  },
  "keywords": ["plvue", "reactivity", "jsx"],
  "author": "yubo9807",
  "homepage": "https://github.com/yubo9807/mvvm_vue3#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yubo9807/mvvm_vue3.git"
  },
  "files": ["plvue", "publish"],
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@types/node": "^20.4.2",
    "sass": "^1.58.3",
    "vite": "^4.4.4"
  }
}
